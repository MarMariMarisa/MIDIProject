#include "reader.h"
#include "UART.h"
#include "printf.h"
#include "LED.h"

uint8_t read_line(){
	//read one characterA
	//print that character on it's own line
	char ch = (char)USART_Read(USART2);
	char toReturn[128];
	int cnt = 1;
	while(ch != 'n'){
		ch = (char)USART_Read(USART2);
		cnt++;
		print(ch);
		toReturn[cnt] = ch;
	}

	return toReturn;
}
