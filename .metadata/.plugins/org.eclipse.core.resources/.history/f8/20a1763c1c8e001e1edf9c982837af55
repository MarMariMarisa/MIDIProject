//print 1 when the wave is high and 0 when the wave is low
//run for a 1 second frequency wave
//verify it works in your terminal


//frequency is in Hertz
#include "printf.h"
#include "systick.h"

void run_wave(int frequency){
	int cnt = 0;
	while(0 == 0){
		while(cnt <=500){
	       cnt++;
		   delay_systick();
		   printf("\r\n%d",0);
			   cnt = 0;
		}
		cnt = 0;
		printf("\r\n");
		while(cnt <=500){
			       cnt++;
				   delay_systick();
				   printf("\r\n%d",1);
		}
		cnt = 0;
	}
}
