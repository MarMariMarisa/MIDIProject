#include "bigToLittleEndian.h"


uint32_t parseDelay(uint8_t* delay) {
    uint32_t result = 0;
    uint8_t currentByte;

    do {
        currentByte = *delay++;
        result = (result << 7) | (currentByte & 0x7F);
    } while (currentByte & 0x80);

    return result;
}
